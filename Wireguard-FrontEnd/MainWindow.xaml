<Window x:Class="Wireguard_FrontEnd.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Wireguard_FrontEnd"
        mc:Ignorable="d"
        Title="MainWindow" Height="178" Width="286">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="97*"/>
            <ColumnDefinition Width="321*"/>
            <ColumnDefinition Width="32*"/>
        </Grid.ColumnDefinitions>
        <ComboBox x:Name="ConfFileComboBox" HorizontalAlignment="Left" Margin="94,10,0,0" VerticalAlignment="Top" Width="120" Text="" SelectionChanged="ConfFileComboBox_SelectionChanged" Height="20" Grid.ColumnSpan="2" Grid.Column="1"/>
        <Button x:Name="StartTunnel" Grid.Column="1" Content="Start Tunnel" HorizontalAlignment="Left" Margin="21,10,0,0" VerticalAlignment="Top" Click="Start_Tunnel_Click" Height="20" Width="68"/>
        <Button x:Name="StopTunnel" Content="Stop Tunnel" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Height="20" Width="68" Click="StopTunnel_Click" Grid.ColumnSpan="2"/>

    </Grid>
</Window>
